{
  "name": "react-mazer-ui",
  "version": "0.1.1-alpha2",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fachryansyah/react-mazer-ui"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@fontsource/nunito": "^4.5.4",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@icon/dripicons": "^2.0.0-alpha.3",
    "bootstrap": "5.2.0",
    "bootstrap-icons": "^1.9.1",
    "perfect-scrollbar": "^1.5.5",
    "popper.js": "^1.16.1",
    "feather-icons": "^4.29.0"
  },
  "dependencies": {
    "@fontsource/nunito": "^4.5.4",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@icon/dripicons": "^2.0.0-alpha.3",
    "@popperjs/core": "^2.9.2",
    "@storybook/addon-storyshots": "^6.3.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "5.2.0",
    "bootstrap-icons": "^1.9.1",
    "feather-icons": "^4.29.0",
    "perfect-scrollbar": "^1.5.5",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "clsx": "^1.2.1",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "exports": {
    ".": {
      "import": "./dist/react-mazer-ui.es.js",
      "require": "./dist/react-mazer-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/react-mazer-ui.es.js",
  "module": "./dist/react-mazer-ui.umd.js",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@vitejs/plugin-react": "^1.1.1",
    "cross-env": "^7.0.3",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "vite": "3.1.7"
  }
}
